<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tickolo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="16" />
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tickolo.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
         android:name="com.example.tickolo.Page2"
            android:label="Page 2"   >
        </activity>
        <activity android:name="com.example.tickolo.Page0" android:label="Tickolo"/>
        <activity android:name="com.example.tickolo.Page1" android:label="Buy"/>
        <activity android:name="com.example.tickolo.Sell" android:label="Sell"/>
        <activity android:name="com.example.tickolo.Modify" android:label="Modify"/>
        <activity android:name="com.example.tickolo.ContactSeller" android:label="Contact Seller"/>
        <activity android:name="com.example.tickolo.Report" android:label="Report Seller"/>
        <activity android:name="com.facebook.LoginActivity" android:label="@string/app_name"/>
         <meta-data android:name="com.facebook.sdk.ApplicationId" android:value="@string/app_id"/>
        
         
    </application>

</manifest>
